{
  "pluginName": "hashids",
  "description": "hashids的加密及解密",
  "version": "0.1.0",
  "preload": "dist/preload.js",
  "author": "anhoder",
  "homepage": "https://anhoder.github.io",
  "logo": "icon.png",
  "platform": ["win32", "darwin", "linux"],
  "features": [
    {
      "code": "add",
      "explain": "hashids添加配置",
      "cmds": ["hadd", "hash添加配置", "hash-add"],
      "icon": "icon.png"
    },
    {
      "code": "encode",
      "explain": "hashids编码",
      "cmds": [
        "haen",
        "hash-encode",
        "hash编码",
        {
          "type": "regex",
          "label": "hashids编码",
          "match": "/^\\d+$/i",
          "minLength": 1
        }
      ],
      "icon": "icon.png"
    },
    {
      "code": "decode",
      "explain": "hashids解码",
      "cmds": [
        "hade",
        "hash-decode",
        "hash解码",
        {
          "type": "regex",
          "label": "hashids解码",
          "match": "/^[A-Za-z0-9]+$/i",
          "minLength": 2
        }
      ],
      "icon": "icon.png"
    }
  ],
  "pluginSetting": {
    "single": true
  }
}
